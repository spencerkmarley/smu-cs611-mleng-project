    "## Convert to geopandas dataframe\n",
    "grid_df['geometry'] = grid_df.Coordinates.apply(wkt.loads)\n",
    "gdf = gpd.GeoDataFrame(grid_df, geometry='geometry')\n",
    "gdf.drop('Coordinates', inplace=True, axis=1)\n",
    "gdf"
